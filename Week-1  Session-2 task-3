  <h1>Task 3: Break and Continue Usage</h1>
  <p>Check the console to observe loop behavior.</p>

  <script>

console.log("=== Break Example: Stop at first prime number ===");
let numbers = [4, 6, 8, 9, 11, 15, 17];

function isPrime(num) {
  if (num < 2) return false;
  for (let i = 2; i <= Math.sqrt(num); i++) {
    if (num % i === 0) return false;
  }
  return true;
}

for (let num of numbers) {
  if (isPrime(num)) {
    console.log("First prime number found:", num);
    break; // Exit the loop once a prime is found
  }
}

console.log("\n=== Continue Example: Print only odd numbers (1â€“20) ===");
for (let i = 1; i <= 20; i++) {
  if (i % 2 === 0) {
    continue; // Skip even numbers
  }
  console.log(i);
}
<script>
